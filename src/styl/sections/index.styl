////////////////////////////////////////////
          /* Maquinaria Section */
////////////////////////////////////////////

.maquinaria-section
  width 91%
  margin-left 5%
  margin-right 5%
  margin-top -60px
  height 350px
  background-image url('../img/group-18.png')
  background-size cover
  background-repeat no-repeat
  color white
  position relative
  box-shadow 0 34px 40px 0 rgba(0, 0, 0, 0.5)


.maquinaria-text-container
  margin-left 85px
  display flex
  flex-direction column
  justify-content center

#maquinaria-title
  font-size 5vw
  margin-top 90px
  margin-bottom 10px

#maquinaria-text
  font-size 1.5vw
  line-height 1.8em
  margin-top 10px
  display inline-block

#maquinaria-cotizar-button
  margin-top 30px
  font-size 1em
  color white
  padding 0 3px
  border-bottom solid 2px #196d2a
  display inline-block
  transition 0.3s
  line-height 18px
  letter-spacing 1px
  width 64px

.maquinaria-cards-container
  position absolute
  height 70%
  width 40%
  top 15%
  right 12%

.maquinaria-card-column-1
  width 100%
  height 50%


.maquinaria-card-column-2
  width 100%
  height 50%


.maquinaria-card
  width 40%
  height 90%
  background-color white
  color black
  padding 0 0 20px 20px

.maquinaria-card-text-container
  height 100%
  display flex
  flex-direction column
  justify-content flex-end

.maquinaria-card-text-container h3
  font-size 1em
  margin-top 10px

.maquinaria-card-text-container p
  font-size 15px
  color #9b9b9b

.top-text p
  bottom 50px


.maquinaria-card-text-container img
  position absolute
  right 0
  top 0
  padding 25px 25px 0px 0px
  height 50px
  width 50px

.maquinaria-image-container
    position absolute
    height 50px
    width 30%
    float right
    top 0
    right 0

