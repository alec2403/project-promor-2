////////////////////////////////////////////
          /* Product Section */
////////////////////////////////////////////

.product-section
  position relative
  height 550px
  display flex


.product-rectangle
  position absolute
  right 0
  background-color #ffffff
  width 63vw
  margin-top -90px

.product-side
  position relative
  width 37vw
  background-image url('../img/group-7.png')
  background-repeat no-repeat
  background-size cover
  z-index -1

.product-text-container
  position relative
  margin-top 90px
  margin-bottom 45px
  margin-left 60px


#product-title
  font-size 4vw
  padding-bottom 9px
  letter-spacing 1.2px

#product-text
  font-size 1.4vw
  color #777777

.slick-center
  transition 0.7s
  transform scale(1.5) !important
  height 220% !important

.slick-track
  display flex !important
  min-height 100% !important

#client-carousel .slick-track .slick-slide
  display flex !important
  align-items center !important
  justify-content center !important
  max-height 100px !important
  margin-top 40px !important
  margin 0 2.8rem !important

#client-carousel .slick-track
  height 170px !important

.slider-buttons
  width 80px
  position absolute
  left -147px
  top 200px
  transition 0.8s
  cursor pointer

.item
  padding 0px 5px

.prev
  position absolute
  left 0
  height 35px
  transition 0.8s

.prev:hover
  left -10px

.next
  position absolute
  right 0
  height 35px
  transition 0.8s

.next:hover
  right -10px

.testin
  width 130px !important


#product-cotizar-button
  margin-left 8vw
  margin-top 60px
  position absolute
  font-size 1em
  color black
  border-bottom solid 2px #196d2a
  transition 0.3s
  padding 0 3px
  line-height 18px
  letter-spacing 1px

