*
    margin 0
    padding 0



////////////////////////////////////////////
          /* Navbar Styles */
////////////////////////////////////////////
.nav
  background-color rgba (0)
  display flex
  height 80px
  justify-content space-between
  align-items center
  border-bottom 0.1px solid #84958B
  background-size cover
  position fixed
  width 100%
  z-index 99

.nav.scrolled
  background-color #0F4627
  border none


.contact-nav
  background-image url('../img/Menu-2.jpg')

.nav a
  color white
  padding 10px 10px 0px 10px
  margin-right 35px
  transition 0.3s

#nav-logo
  min-width 120px
  min-height 40px

#phone-nav-a
  margin-right 100px

#phone-nav
  margin-right 8px
  margin-bottom -4px


.nav-logo-container
  padding-left 70px


.nav-content span
  padding 0px 10px

.nav a img
  height 20px
  width 20px


.nav-click:hover
  color #26963d


/* Extra small devices (phones, 600px and down) */
@media only screen and (max-width: 600px)
    .nav
      flex-direction column
      height 100px
      padding-bottom 15px

    .nav-logo-container
        padding-top 10px
        padding-left 0px

    .nav-content-container
      margin-right auto
      margin-left auto

    .nav a
      margin-right 0

    #phone-nav-a
      margin-right 0

